{
  "name": "free-node-web",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=free-node-web",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name=free-node-web --branch=main",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name=free-node-web --branch=preview",
    "pub": "wrangler pages deploy dist --project-name=free-node-web",
    "pub:prod": "wrangler pages deploy dist --project-name=free-node-web --branch=main",
    "deploy:mail": "cd workers/ghost-mail && npm run deploy",
    "deploy:news": "cd workers/news-server && npm run deploy",
    "deploy:oracle": "cd workers/ghost-oracle && npm run deploy",
    "db:init": "cd workers/ghost-mail && npm run db:init"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.11",
    "@types/react-router-dom": "^5.3.3",
    "openapi-fetch": "^0.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1",
    "use-sound": "^5.0.0",
    "viem": "^2.40.3",
    "wagmi": "^2.12.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^18.15.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.0",
    "wrangler": "^4.51.0"
  }
}
